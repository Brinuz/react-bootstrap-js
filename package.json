{
  "name": "bootstrap-react-app",
  "repository": {
    "type": "git",
    "url": "git://github.com/Brinuz/react-bootstrap-js.git"
  },
  "version": "1.0.0",
  "description": "React JS Bootstrap",
  "main": "index.js",
  "scripts": {
    "update": "yarn outdated && yarn upgrade --latest && yarn audit fix",
    "lint": "node node_modules/eslint/bin/eslint.js \"src/**\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "test:verbose": "jest --verbose",
    "test:commit": "yarn lint && yarn test:verbose && yarn outdated",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --hot --open --devtool source-map"
  },
  "author": "Bruno Monteiro",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-html": "^6.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest-dom": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-testing-library": "^3.0.3",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
